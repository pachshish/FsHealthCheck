{
  "id": null,
  "uid": "fs-health",
  "title": "Filesystem Health",
  "tags": [ "filesystem", "health", "fs_health" ],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 4,
  "refresh": "30s",
  "style": "dark",
  "editable": true,
  "panels": [
    {
      "type": "timeseries",
      "title": "Free & Used space (%)",
      "id": 1,
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 7
      },
      "targets": [
        {
          "expr": "(1 - fs_share_free_ratio) * 100",
          "legendFormat": "used {{share}}",
          "refId": "A"
        },
        {
          "expr": "fs_share_free_ratio * 100",
          "legendFormat": "free {{share}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 80
              },
              {
                "color": "red",
                "value": 90
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "type": "timeseries",
      "title": "Used vs Free (GB)",
      "id": 2,
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 7
      },
      "targets": [
        {
          "expr": "(fs_share_total_bytes - fs_share_free_bytes) / 1024 / 1024 / 1024",
          "legendFormat": "used {{share}}",
          "refId": "A"
        },
        {
          "expr": "fs_share_free_bytes / 1024 / 1024 / 1024",
          "legendFormat": "free {{share}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "gbytes",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "type": "timeseries",
      "title": "Read & Write throughput (MB/s)",
      "id": 5,
      "gridPos": {
        "x": 0,
        "y": 7,
        "w": 24,
        "h": 7
      },
      "targets": [
        {
          "expr": "fs_share_write_throughput_bytes / 1024 / 1024",
          "legendFormat": "write {{share}}",
          "refId": "A"
        },
        {
          "expr": "fs_share_read_unbuffered_throughput_bytes / 1024 / 1024",
          "legendFormat": "read (unbuffered) {{share}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "MBps",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "type": "timeseries",
      "title": "Small files ops/sec (create & delete)",
      "id": 7,
      "gridPos": {
        "x": 0,
        "y": 14,
        "w": 24,
        "h": 7
      },
      "targets": [
        {
          "expr": "fs_share_smallfiles_create_ops_per_sec",
          "legendFormat": "create {{share}}",
          "refId": "A"
        },
        {
          "expr": "fs_share_smallfiles_delete_ops_per_sec",
          "legendFormat": "delete {{share}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "type": "timeseries",
      "title": "Small IO latency (ms)",
      "id": 8,
      "gridPos": {
        "x": 0,
        "y": 21,
        "w": 12,
        "h": 7
      },
      "targets": [
        {
          "expr": "fs_share_small_read_latency_ms",
          "legendFormat": "read {{share}}",
          "refId": "A"
        },
        {
          "expr": "fs_share_small_write_latency_ms",
          "legendFormat": "write {{share}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "type": "timeseries",
      "title": "Directory listing duration (s)",
      "id": 9,
      "gridPos": {
        "x": 12,
        "y": 21,
        "w": 12,
        "h": 7
      },
      "targets": [
        {
          "expr": "fs_share_dirlist_duration_seconds",
          "legendFormat": "{{share}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "type": "timeseries",
      "title": "I/O errors (per minute)",
      "id": 10,
      "gridPos": {
        "x": 0,
        "y": 28,
        "w": 24,
        "h": 7
      },
      "targets": [
        {
          "expr": "rate(fs_share_io_errors_total[5m]) * 60",
          "legendFormat": "{{share}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "type": "timeseries",
      "title": "Connection open latency (ms)",
      "id": 11,
      "gridPos": {
        "x": 0,
        "y": 42,
        "w": 24,
        "h": 7
      },
      "targets": [
        {
          "expr": "fs_share_connection_open_latency_ms",
          "legendFormat": "{{share}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "red",
                "value": 150
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": { "mode": "single" }
      }
    }

  ],
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [ "5s", "10s", "30s", "1m", "5m", "15m" ]
  }
}
