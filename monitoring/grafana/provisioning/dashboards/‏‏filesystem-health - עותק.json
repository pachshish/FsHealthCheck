{
  "id": null,
  "uid": "fs-health",
  "title": "Filesystem Health",
  "tags": [ "filesystem", "health", "fs_health" ],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "30s",
  "style": "dark",
  "editable": true,
  "panels": [
    {
      "type": "timeseries",
      "title": "Free space (%)",
      "id": 1,
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "fs_share_free_ratio * 100",
          "legendFormat": "{{share}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "red",
                "value": 10
              },
              {
                "color": "yellow",
                "value": 20
              },
              {
                "color": "green",
                "value": 30
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "type": "timeseries",
      "title": "Capacity (GB)",
      "id": 2,
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "fs_share_total_bytes / 1024 / 1024 / 1024",
          "legendFormat": "total {{share}}",
          "refId": "A"
        },
        {
          "expr": "fs_share_free_bytes / 1024 / 1024 / 1024",
          "legendFormat": "free {{share}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "gbytes",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "type": "timeseries",
      "title": "Write throughput (MB/s)",
      "id": 3,
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "fs_share_write_throughput_bytes / 1024 / 1024",
          "legendFormat": "{{share}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "MBps",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "type": "timeseries",
      "title": "Read throughput (unbuffered MB/s)",
      "id": 4,
      "gridPos": {
        "x": 12,
        "y": 8,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "fs_share_read_unbuffered_throughput_bytes / 1024 / 1024",
          "legendFormat": "{{share}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "MBps",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "type": "timeseries",
      "title": "Small files ops/sec (create & delete)",
      "id": 5,
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 24,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "fs_share_smallfiles_create_ops_per_sec",
          "legendFormat": "create {{share}}",
          "refId": "A"
        },
        {
          "expr": "fs_share_smallfiles_delete_ops_per_sec",
          "legendFormat": "delete {{share}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": { "mode": "single" }
      }
    }
  ],
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m"
    ]
  }
}
